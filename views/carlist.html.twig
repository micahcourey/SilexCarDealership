<!DOCTYPE html>
<html>
  <head>
    <title>List of All Cars</title>
  </head>
  <body>
    <h1>List of All Cars</h1>
    <ul>
        {% for car in car_list %}
            <img src= {{ car.getPicture }}>
            <li>{{ car.getMake }}</li>
            <li>{{ car.getPrice }}</li>
            <li>{{ car.getMiles }}</li>
        {% endfor %}
    </ul>
  </body>
</html>
